<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/root_element"
    tools:context=".UserInfoactivity">

    <EditText
        android:id="@+id/Email_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:focusable="false"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/name_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:focusable="false"
        android:inputType="text"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/lastname_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="24dp"
        android:ems="10"
        android:focusable="false"
        android:inputType="text"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Phone_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:focusable="false"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Password_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="316dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:focusable="false"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/batton_back"
        android:layout_width="118dp"
        android:layout_height="64dp"
        android:layout_marginTop="492dp"
        android:layout_marginEnd="20dp"
        android:text="назад"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/batton_edit"
        android:layout_width="199dp"
        android:layout_height="65dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="180dp"
        android:text="редактировать данные"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/batton_del"
        android:layout_width="197dp"
        android:layout_height="64dp"
        android:layout_marginTop="492dp"
        android:layout_marginEnd="180dp"
        android:text="удалить аккаунт"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:layout_width="126dp"
        android:layout_height="47dp"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="268dp"
        android:ems="10"
        android:focusable="false"
        android:text="Телефон:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:layout_width="126dp"
        android:layout_height="47dp"
        android:layout_marginTop="316dp"
        android:layout_marginEnd="268dp"
        android:ems="10"
        android:focusable="false"
        android:text="Пароль:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:layout_width="126dp"
        android:layout_height="47dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="268dp"
        android:ems="10"
        android:focusable="false"
        android:text="Email/login:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:layout_width="126dp"
        android:layout_height="47dp"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="268dp"
        android:ems="10"
        android:focusable="false"
        android:text="Фамилия:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:layout_width="126dp"
        android:layout_height="47dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="268dp"
        android:ems="10"
        android:focusable="false"
        android:text="Имя:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>